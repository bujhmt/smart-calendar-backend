{
    "name": "smart-calendar-backend",
    "version": "2.0.0",
    "main": "dist/app.js",
    "scripts": {
        "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
        "watch-ts": "tsc -w",
        "dev": "cross-env NODE_ENV=dev concurrently \"npm run prettier-watch\" \"npm run watch-ts\" \"nodemon ./dist/app.js\"",
        "build": "tsc",
        "heroku-postbuild": "npm install && npm run build",
        "start": "node ./dist/app.js"
    },
    "engines": {
        "node": "14.x"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "class-validator": "^0.13.1",
        "cloudinary": "^1.24.0",
        "concurrently": "^5.3.0",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-validator": "^5.3.1",
        "fs-extra": "^9.1.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.10.11",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "uuid": "^8.3.2"
    },
    "author": "bujhmt",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@types/fs-extra": "^9.0.7",
        "@types/multer": "^1.4.5",
        "@types/uuid": "^8.3.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/config": "0.0.36",
        "@types/express": "^4.16.1",
        "@types/express-validator": "^3.0.0",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/mongoose": "^5.7.37",
        "@types/node": "^14.11.1",
        "nodemon": "^2.0.4",
        "onchange": "^7.0.2",
        "prettier": "^2.1.2",
        "tslint": "^6.1.3",
        "typescript": "^4.0.2"
    }
}
